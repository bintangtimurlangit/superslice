# SuperSlice API Configuration
# Copy this file to .env and adjust values as needed

# Directory paths (inside container)
UPLOAD_DIR=/app/uploads
OUTPUT_DIR=/app/output

# PrusaSlicer binary path
PRUSASLICER_PATH=/slic3r/squashfs-root/usr/bin/prusa-slicer

# Processing limits
SLICE_TIMEOUT=120
MAX_FILE_SIZE=104857600

# CORS settings (comma-separated origins, use * for all)
CORS_ORIGINS=*
